   "source": [
    "def combination(data, number):\n",
    "    \n",
    "    set = []\n",
    "    \n",
    "    if number <= 1:\n",
    "        return data\n",
    "\n",
    "    for m in range(len(data)):\n",
    "        for n in combination(data[m+1:], number - 1):\n",
    "            set.append(data[m] + n)\n",
    "    \n",
    "    return set\n",
    "\n",
    "\n"
   ]
