   "source": [
    "def combination(data, number, value = ''):\n",
    "    combi = value\n",
    "    if number >= 1:\n",
    "        for i in data:\n",
    "            combi = value + i\n",
    "            combination(data, number - 1, combi)\n",
    "\n",
    "    else:\n",
    "        print(combi)\n",
    "        return combi\n"
   ]
